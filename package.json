{
  "name": "webpack-generator",
  "version": "1.0.0",
  "description": "Simple webpack generator CLI",
  "main": "index.js",
  "author": "Reece Lucas <reecelucas@sky.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reecelucas/webpack-generator.git"
